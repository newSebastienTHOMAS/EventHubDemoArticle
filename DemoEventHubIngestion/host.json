{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "Default": "Information"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": false
      }
    }
  },

  "extensions": {
    "eventHubs": {
      "batchCheckpointFrequency": 1,
      "eventProcessorOptions": {
        "maxBatchSize": 2048,
        "prefetchCount": 4096
      }
    }
  }
}